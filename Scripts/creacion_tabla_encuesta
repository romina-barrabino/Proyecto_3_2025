CREATE TABLE dbo.Encuesta (
    id_encuesta INT IDENTITY(1,1) PRIMARY KEY,
    id_empleado INT,
    fecha DATE DEFAULT GETDATE(),
    p1 INT,
    p2 INT,
    p3 INT,
    p4 INT,
    p5 INT,
    p6 INT,
    p7 INT,
    p8 INT,
    p9 INT,
    p10 INT,
    p11 INT,
    p12 INT,
    FOREIGN KEY (id_empleado) REFERENCES dbo.Informacion(id_empleado));
GO
